		System.out.println("3-DeleteList()测试结果：");
		System.out.println("DeleteList()前的链表为：");
		System.out.println(list);
		list.DeleteList(list);
		System.out.println("DeleteList()后的链表为：");
		System.out.println(list);
	
		
